{
	"name": "Dream Utility sidebar",
	"manifest_version": 2,
	"description": "Dream Utility Sidebar",
	"version": "0.2",
	"background": {
		"page": "background.html"
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_title": "Dream utility sidebar"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["vendor/bootstrap/js/bootstrap.min.js", "vendor/jquery-1.11.1.js", "script.js"],
			"css": [ "css/styles.css", "vendor/bootstrap/css/bootstrap.min.css" ]
		}
	],
        "web_accessible_resources": [ "css/styles.css", "vendor/bootstrap/css/bootstrap.min.css" ],
	"permissions": [ "tabs" ]
}
